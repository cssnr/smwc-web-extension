{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true,
    "webextensions": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-empty": "off",
    "no-extra-semi": "off",
    "no-func-assign": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
